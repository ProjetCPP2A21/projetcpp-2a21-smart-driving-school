     <string notr="true">QTimeEdit {
    border: 2px solid qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #43cea2, stop:1 #185a9d);
    border-radius: 10px;
    padding: 4px 8px;
    background: white;
    color: #2c3e50;
    min-width: 100px;
    max-width: 150px;
    height: 24px;
    selection-background-color: #43cea2; /* couleur quand on sélectionne une partie de l'heure */
    selection-color: white;
}

/* Boutons d’incrémentation/décrémentation */
QTimeEdit::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 16px;
    border: none;
    background: transparent;
}

QTimeEdit::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    width: 16px;
    border: none;
    background: transparent;
}

QTimeEdit::up-arrow, QTimeEdit::down-arrow {
    width: 10px;
    height: 10px;
}

/* Focus */
QTimeEdit:focus {
    border: 2px solid qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #ff4b1f, stop:1 #ffc837);
    /* ⚠️ box-shadow n’est pas supporté par Qt StyleSheets */
}

/* Hover */
QTimeEdit:hover {
    border: 2px solid #43cea2;
}</string>
