    background: white;
    color: #2c3e50;
    min-width: 100px;       /* largeur minimale plus petite */
    max-width: 150px;       /* facultatif : limite largeur */
    height: 24px;           /* hauteur plus petite */
}

/* Drop-down (flèche) */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;           /* réduit largeur flèche */
    border: none;
    background: transparent;
}

QComboBox::down-arrow {
    width: 12px;
    height: 12px;
}

/* Hover et focus restent inchangés */
QComboBox:focus {
    border: 2px solid qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #ff4b1f, stop:1 #ffc837);
    box-shadow: 0 0 4px rgba(255, 107, 53, 0.6);
}

QComboBox:hover {
    border: 2px solid #43cea2;
    box-shadow: 0 0 4px rgba(67, 206, 162, 0.6);
}

